{
  "name": "adt/nette-application-components",
  "description": "nette/application components",
  "keywords": [
    "nette",
    "application",
    "presenter",
    "component",
    "control"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/AppsDevTeam/nette-application-components",
  "authors": [
    {
      "name": "Apps Dev Team",
      "homepage": "https://www.appsdevteam.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "nette/application": "^3.2.6 || ^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "ADT\\Application\\": "src"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}